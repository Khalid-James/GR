<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data Overview</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>Users</h1>
    <ul>
        <% users.forEach(user => { %>
            <li><%= user.name %></li>
        <% }) %>
    </ul>

    <h1>Posts</h1>
    <ul>
        <% posts.forEach(post => { %>
            <li><%= post.title %>: <%= post.content %></li>
        <% }) %>
    </ul>

    <h1>Comments</h1>
    <ul>
        <% comments.forEach(comment => { %>
            <li>Post ID <%= comment.postId %>: <%= comment.content %></li>
        <% }) %>
    </ul>
</body>
</html>
